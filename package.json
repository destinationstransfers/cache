{
  "name": "@destinationstransfers/cache",
  "version": "1.3.0",
  "description": "Fast, fault-tolerant, disk-based, data-agnostic cache.",
  "main": "index.js",
  "scripts": {
    "test": "node --expose-gc node_modules/jest/bin/jest --detectOpenHandles --detectLeaks --notify --logHeapUsage",
    "typecheck": "tsc --p jsconfig.json"
  },
  "engines": {
    "node": ">=10.7"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/destinationstransfers/cache.git"
  },
  "author": "Tino Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/destinationstransfers/cache/issues"
  },
  "homepage": "https://github.com/destinationstransfers/cache#readme",
  "dependencies": {
    "ssri": "^6.0.1"
  },
  "devDependencies": {
    "@destinationstransfers/eslint-config": "5.3.0",
    "@types/node": "10.12.23",
    "@types/ssri": "6.0.0",
    "eslint": "latest",
    "jest": "24.1.0",
    "jest-junit": "6.2.1",
    "rimraf": "2.6.3",
    "stream-buffers": "3.0.2",
    "typescript": "3.3.3",
    "weak": "1.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "verbose": true,
    "bail": true,
    "coverageReporters": [
      "text",
      "json",
      "html",
      "cobertura"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@destinationstransfers"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    }
  }
}
