{
  "name": "@destinationstransfers/cache",
  "version": "2.2.1",
  "description": "Fast, fault-tolerant, disk-based, data-agnostic cache.",
  "main": "index.js",
  "scripts": {
    "test": "node --expose-gc node_modules/jest/bin/jest --detectOpenHandles --detectLeaks --notify --logHeapUsage",
    "typecheck": "tsc --p jsconfig.json",
    "lint": "eslint \"**/*.js\" --ignore-path .gitignore"
  },
  "engines": {
    "node": ">=10.7"
  },
  "files": [
    "index.js",
    "index.d.ts"
  ],
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/destinationstransfers/cache.git"
  },
  "author": "Tino Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/destinationstransfers/cache/issues"
  },
  "homepage": "https://github.com/destinationstransfers/cache#readme",
  "dependencies": {
    "ssri": "^6.0.1"
  },
  "devDependencies": {
    "weak": "1.0.1",
    "@destinationstransfers/eslint-plugin": "2.3.5",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.10",
    "@types/ssri": "6.0.1",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "eslint": "latest",
    "jest": "24.7.1",
    "jest-junit": "6.4.0",
    "stream-buffers": "3.0.2",
    "typescript": "3.4.5"
  },
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node",
    "verbose": true,
    "bail": true,
    "coverageReporters": [
      "text",
      "json",
      "html",
      "cobertura"
    ]
  }
