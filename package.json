{
  "name": "cache",
  "version": "1.0.0",
  "cache-version": {
    "content": "5",
    "index": "10"
  },
  "description": "Fast, fault-tolerant, disk-based, data-agnostic, content-addressable cache.",
  "main": "index.js",
  "scripts": {
    "test": "tap --coverage --nyc-arg=--all -J test/*.js",
    "benchmarks": "node test/benchmarks"
  },
  "engines": {
    "node": ">=10.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/destinationstransfers/cache.git"
  },
  "author": "Tino Vyatkin <tino@vtkn.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/destinationstransfers/cache/issues"
  },
  "homepage": "https://github.com/destinationstransfers/cache#readme",
  "dependencies": {
    "chownr": "^1.1.1",
    "fast-glob": "^2.2.6",
    "figgy-pudding": "^3.5.1",
    "lru-cache": "^5.1.1",
    "mississippi": "^3.0.0",
    "move-concurrently": "^1.0.1",
    "p-queue": "^3.0.0",
    "rimraf": "^2.6.3",
    "ssri": "^6.0.1",
    "unique-filename": "^1.1.1"
  },
  "devDependencies": {
    "@destinationstransfers/eslint-config": "^5.2.1",
    "bluebird": "^3.5.3",
    "eslint": "latest",
    "require-inject": "^1.4.4",
    "tacks": "^1.3.0",
    "tap": "^12.5.1"
  },
  "config": {
    "nyc": {
      "exclude": [
        "node_modules/**",
        "test/**"
      ]
    }
  },
  "eslintConfig": {
    "extends": [
      "@destinationstransfers"
    ],
    "env": {
      "node": true
    },
    "parserOptions": {
      "sourceType": "script"
    },
    "rules": {
      "require-await": "off"
    }
  }
}
